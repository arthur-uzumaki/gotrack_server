{
  "openapi": "3.0.3",
  "info": {
    "title": "go track",
    "version": "1.0.0"
  },
  "components": {
    "schemas": {}
  },
  "paths": {
    "/sessions": {
      "post": {
        "tags": [
          "User"
        ],
        "description": "Authenticate user google",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "tokenId": {
                    "type": "string"
                  }
                },
                "required": [
                  "tokenId"
                ],
                "additionalProperties": false
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "Default Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "accessToken": {
                      "type": "string",
                      "pattern": "^[A-Za-z0-9-_]+\\.[A-Za-z0-9-_]+\\.[A-Za-z0-9-_]*$"
                    }
                  },
                  "required": [
                    "accessToken"
                  ],
                  "additionalProperties": false
                }
              }
            }
          }
        }
      }
    }
  }
}